        <listener class-name="util.ExtentReportManager"/>
    </listeners>
    <test name="higo.id and blog.higo.id test">
        <groups>
            <run>
                <include name="smoke"/>
            </run>
        </groups>
        <classes>
            <class name="HeaderNavTest"/>
            <class name="FooterTest"/>
            <class name="AboutHigoTest"/>
            <class name="AnnualReportTest"/>
            <class name="WifiAdvertisingTest"/>
            <class name="CaseStudyTest"/>
            <class name="ContactHigoTest"/>
            <class name="BlogHigoTest"/>
            <class name="BlogHigoDetailTest"/>
        </classes>
    </test>
</suite>